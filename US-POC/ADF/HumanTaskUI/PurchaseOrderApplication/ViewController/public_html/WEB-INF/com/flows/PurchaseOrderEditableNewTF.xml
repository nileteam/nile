<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="PurchaseOrderEditableNewTF">
    <default-activity>Load</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>PurchaseOrderBean</managed-bean-name>
      <managed-bean-class>com.view.beans.PurchaseOrder</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="purchaseOrderEditableNewFragment">
      <page>/WEB-INF/com/fragments/purchaseOrderEditableNewFragment.jsff</page>
    </view>
    <method-call id="Load">
      <method>#{pageFlowScope.PurchaseOrderBean.E_Load}</method>
      <outcome>
        <fixed-outcome>GO</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__2">
      <from-activity-id>Load</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>GO</from-outcome>
        <to-activity-id>purchaseOrderEditableNewFragment</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
